{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
  },
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "vars-on-top": 0,
    "consistent-return": 0,
    "prefer-arrow-callback": 0,
    "prefer-template": 0,
    "object-shorthand": 0,
    "no-console": 0,
    "no-unused-vars": 0,
    "block-spacing": 0,
    "no-plusplus": 0,
    "comma-dangle": 0,
    "arrow-body-style": ["error", "as-needed"],
    "no-confusing-arrow": 0,
    "default-case": 0,
    "arrow-parens": 0,
    "no-return-assign": 0,
    "semi": ["error", "never"],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "import/prefer-default-export": 0,
    "react/jsx-uses-vars": [2]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ]
}
